{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.16"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "daisyui": "^5.3.10",
    "postcss": "^8.5.6",
    "sass": "^1.93.2",
    "tailwindcss": "^4.1.16"
  },
  "scripts": {
    "dev": "concurrently \"npm run daisyui\" \"npm run watch\"",
    "daisyui": "tailwindcss -i ./assets/css/input.css -o ./dist/style.css --watch",
    "watch": "sass --watch assets/scss/main.scss:dist/main.css --style=expanded"
  }
}